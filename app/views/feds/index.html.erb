<p id="notice"><%= notice %></p>

<h1>Feds</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Buyer</th>
      <th>Ate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feds.each do |fed| %>
      <tr>
        <td><%= fed.date %></td>
        <td><%= fed.buyer %></td>
        <td><%= fed.ate %></td>
        <td><%= link_to 'Show', fed %></td>
        <td><%= link_to 'Edit', edit_fed_path(fed) %></td>
        <td><%= link_to 'Destroy', fed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fed', new_fed_path %>
